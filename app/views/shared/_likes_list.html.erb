
<% if @likes_list.blank? %>
	<li class="dropdown-header">No tienes favoritos todav√≠a</li>
<% else %>
	<li class="dropdown-header">Mis favoritos</li>
	<% @likes_list.each do |like| %>
		<li>
			<%= link_to like.pin.title, pin_path(like.pin) %>
		</li>
		
	<% end %>
	<li role="separator" class="divider"></li>
	<li>
		<%= link_to 'Ver todos', likes_path %>
	</li>
<% end %>
